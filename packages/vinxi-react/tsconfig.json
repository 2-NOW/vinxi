{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"jsx": "react-jsx",
		"isolatedModules": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"outDir": "dist"
	}
}
