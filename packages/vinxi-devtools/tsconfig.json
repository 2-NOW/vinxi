{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"checkJs": true,
		"noEmit": false,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"isolatedModules": true,
		"strict": true,
		"declarationMap": true,
		"declaration": true,
		"skipLibCheck": true,
		"emitDeclarationOnly": true,
		"types": ["vinxi/types/client"],
		"outDir": "./out/types"
	},
	"include": ["./mount.jsx", "./index.js"]
}
