{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"isolatedModules": true,
		"types": ["react/experimental", "react-dom/experimental"]
	}
}
